{
  "name": "bup-dockerized",
  "version": "1.0.0",
  "description": "",
  "main": "bin/index.js",
  "scripts": {
    "start": "node dist/server/index.js",
    "build": "NODE_ENV=production webpack",
    "watch-client": "NODE_ENV=development webpack --watch",
    "build-server": "tsc --project src/server/tsconfig.json",
    "watch-server": "concurrently \"tsc --watch --project src/server/tsconfig.json\" \"npx nodemon dist/server/index.js\"",
    "dev": "concurrently \"npm run watch-client\" \"npm run watch-server\"",
    "docker-dev": ". ./run-dev.sh",
    "tsc": "tsc --noEmit -p ./src/server/tsconfig.json"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cron": "^1.8.2",
    "cron-parser": "^4.1.0",
    "date-fns": "^2.25.0",
    "filesize": "^8.0.6",
    "nanoid": "^3.1.30",
    "preact": "^10.5.15",
    "ramda": "^0.27.1",
    "serve-handler": "^6.1.3",
    "source-map-support": "^0.5.20",
    "ws": "^8.14.2",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/cron": "^1.7.3",
    "@types/ramda": "^0.27.49",
    "@types/serve-handler": "^6.1.1",
    "@types/ws": "^8.2.0",
    "concurrently": "^6.4.0",
    "css-loader": "^6.5.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.4.4",
    "ts-loader": "^9.5.0",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
